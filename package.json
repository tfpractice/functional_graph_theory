{
  "name": "functional_graph_theory",
  "version": "1.0.0",
  "description": "a Map based implementation of canonical graph data algorithms",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn run build:umd && yarn run build:es6",
    "build:es6": "NODE_ENV=production rollup -c rollup.es6.config.js",
    "build:umd": "NODE_ENV=production rollup -c rollup.config.js",
    "build:dev": "rollup -c rollup.es6.config.js",
    "test": "jest",
    "doc": "jsdoc -c config/conf.json app",
    "coverage": "yarn test && cat coverage/lcov.info | coveralls",
    "build:wpack": "webpack --progress --color --env.prod -p --config ./config/webpack/config.babel.js",
    "dev": "node .",
    "lint": "./node_modules/.bin/eslint  --fix -c .eslintrc.js . --ext .js --ignore-path .gitignore --cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tfpractice/functional_graph_theory.git"
  },
  "keywords": [
    "algorithms",
    "data-structures",
    "graph-theory",
    "functional-programming"
  ],
  "author": "theory f practice <jackson.brebnor@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tfpractice/functional_graph_theory/issues"
  },
  "homepage": "https://github.com/tfpractice/functional_graph_theory#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-preset-latest": "^6.16.0",
    "clean-webpack-plugin": "^0.1.14",
    "coveralls": "^2.11.15",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-import-resolver-babel-module": "^2.2.1",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "ink-docstrap": "^1.3.0",
    "jasmine": "^2.5.2",
    "jasmine-expect": "^3.0.1",
    "jest": "^18.1.0",
    "jest-cli": "^18.1.0",
    "jsdoc": "^3.4.2",
    "nyc": "^10.0.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.40.0",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-filesize": "^1.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-progress": "^0.2.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-plugin-visualizer": "^0.1.5",
    "webpack": "2.1.0-beta.28"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "check-coverage": true,
    "show-process-tree": true
  },
  "jest": {
    "collectCoverage": "true",
    "collectCoverageFrom": [
      "src/**"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathDirs": [
      "spec/"
    ],
    "unmockedModulePathPatterns": [
      "jasmine-expect"
    ]
  },
  "dependencies": {
    "turmeric": "git://github.com/tfpractice/turmeric.git"
  }
}
